    "environment": {
        "MONGO_INITDB_ROOT_USERNAME": "root",
        "MONGO_INITDB_ROOT_PASSWORD": "example"
    }
}, {
    "name": "MariaDB",
    "description": "One of the most popular open source relational databases.",
